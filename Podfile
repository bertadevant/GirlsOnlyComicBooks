# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'GirlsOnlyComicBooks' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GirlsOnlyComicBooks
	pod 'Alamofire'
    pod 'AlamofireImage'
	pod 'RxSwift'   
    pod 'RxCocoa'
    pod 'RxDataSources'
	pod 'SwiftyJSON' 

  target 'GirlsOnlyComicBooksTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'Alamofire'
    pod 'AlamofireImage'
	pod 'RxSwift'   
    pod 'RxCocoa'
	pod 'SwiftyJSON' 
  end

  target 'GirlsOnlyComicBooksUITests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RxSwift'   
    pod 'RxCocoa'   
  end

end
